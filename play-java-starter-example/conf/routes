# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)



GET     /createaccount              controllers.UserIDController.create()
POST    /createaccount              controllers.UserIDController.save()

GET     /register_voter             controllers.VoterRegistrationController.create()
GET     /register_voter             controllers.VoterRegistrationController.save()